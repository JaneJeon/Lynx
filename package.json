{
  "name": "lynx",
  "description": "Modern, lightweight, and planet-scale link shortener + analytics + management!",
  "repository": "JaneJeon/Lynx",
  "author": "Jane Jeon <JaneJeon9719@gmail.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node bin/www",
    "dev": "run-s build seed && run-p dev:* watch",
    "dev:www": "DEBUG=knex:query,ioredis:* nodemon bin/www",
    "migrate": "knex migrate:latest",
    "migrate:one": "knex migrate:up",
    "migrate:make": "knex migrate:make",
    "migrate:list": "knex migrate:list",
    "rollback": "knex migrate:rollback",
    "rollback:one": "knex migrate:down",
    "seed": "knex seed:run",
    "seed:make": "bash ./scripts/make-seed.sh",
    "build": "run-p migrate build:*",
    "build:yaml": "yaml2json config --save --recursive",
    "watch": "run-p watch:*",
    "watch:yaml": "yarn build:yaml --watch",
    "lint": "run-p lint:* --aggregate-output",
    "lint-fix": "run-p lint:fix:* --aggregate-output",
    "lint:eslint": "eslint .",
    "lint:fix:eslint": "yarn lint:eslint --fix",
    "lint:prettier": "prettier --check '**/*.{js,md,json,yml,yaml,css,html}'",
    "lint:fix:prettier": "yarn lint:prettier --write",
    "pretest": "run-s build seed",
    "test": "jest --forceExit",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@casl/ability": "^3.4.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-session": "^1.4.0",
    "dotenv-defaults": "^1.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-query-boolean": "^2.0.0",
    "express-request-id": "^1.4.1",
    "got": "^10.2.2",
    "helmet": "^3.21.1",
    "http-errors": "^1.7.3",
    "ioredis": "^4.14.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "metascraper": "^5.7.6",
    "metascraper-author": "^5.7.6",
    "metascraper-date": "^5.7.6",
    "metascraper-description": "^5.7.6",
    "metascraper-lang": "^5.7.6",
    "metascraper-logo": "^5.7.6",
    "metascraper-logo-favicon": "^5.7.6",
    "metascraper-publisher": "^5.7.6",
    "metascraper-title": "^5.7.6",
    "ms": "^2.1.2",
    "nanoid": "^2.1.4",
    "normalize-url": "^4.4.1",
    "objection": "^2.0.10",
    "objection-authorize": "^3.1.1",
    "objection-hashid": "^1.2.0",
    "objection-table-name": "^1.2.0",
    "objection-visibility": "^0.4.0",
    "passport": "^0.4.1",
    "passport-slack-fixed": "^0.0.8",
    "pg": "^7.12.1",
    "pino": "^5.13.4",
    "pm2": "^4.2.1",
    "slack": "^11.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.0.7",
    "is-ci": "^2.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "keygrip": "^1.1.0",
    "lint-staged": "^9.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "objection-gen": "^0.5.2",
    "objection-swagger": "^3.1.2",
    "pino-pretty": "^3.2.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  }
}
