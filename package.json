    "objection-table-name": "^1.2.5",
    "openid-client": "^4.9.1",
    "p-timeout": "^4.1.0",
    "pg": "^8.7.1",
    "pino": "^7.0.5",
    "rate-limit-redis": "^2.1.0"
  },
  "devDependencies": {
    "@casl/react": "^2.1.1",
    "@janejeon/eslint-config": "^2.2.0",
    "@janejeon/prettier-config": "^1.1.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/jest": "^27.0.2",
    "ajv": "^8.6.3",
