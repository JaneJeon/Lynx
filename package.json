    "onchange": "^7.1.0",
    "pino-pretty": "^7.1.0",
    "qrcode": "^1.4.4",
    "ra-data-simple-rest": "^3.19.0",
    "react": "^17.0.1",
    "react-admin": "^3.19.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
