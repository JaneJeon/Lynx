    "helmet": "^4.6.0",
    "http-errors": "^1.7.3",
    "ioredis": "^4.28.0",
    "jwks-rsa": "^2.0.5",
    "knex": "^0.21.21",
    "lodash": "^4.17.21",
    "metascraper": "^5.24.9",
    "metascraper-author": "^5.24.9",
    "metascraper-date": "^5.24.9",
